<aura:component controller="CaseService" implements="force:lightningQuickAction,force:appHostable,flexipage:availableForAllPageTypes,force:hasRecordId,force:hasSObjectName">
    <article class="slds-card"></article>
        <aura:handler name="init" value="{!this}" action="{!c.myAction}"/>
        
        <aura:attribute name="duplicateCases" type="List" default=""/>

            <div class="slds-media__body">  
              <h2 class="slds-page-header">
                <a class="slds-card__header-link slds-truncate" title="Cases">
                  <span>DuplicateCases</span>
                </a>
              </h2>              
            </div>
            
            <aura:iteration items="{!v.duplicateCases}" var="duplicateCaseId">     
            <c:caseResultComponent duplicateCaseId="{!duplicateCaseId}" parentCaseId="{!v.recordId}"/>
           
            </aura:iteration>
            

     
</aura:component>	





